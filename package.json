{
   "name": "react-19-template",
   "version": "0.1.0",
   "description": " A template of a React, Babel, and Webpack app using bleeding-edge packages",
   "repository": {
      "type": "git",
      "url": "https://github.com/sai-chai/react-19-template.git"
   },
   "main": "index.js",
   "scripts": {
      "start": "npm run lint && webpack-dev-server --open --mode development",
      "build": "npm run lint && webpack --mode production",
      "format": "prettier --write \"src/**/*.js\"",
      "lint": "eslint \"src/**/*.js\"",
      "test": "jest"
   },
   "keywords": [
      "react",
      "babel",
      "webpack",
      "es6",
      "es2015",
      "node",
      "template"
   ],
   "author": "Sai Chimata @sai-chai",
   "license": "MIT",
   "devDependencies": {
      "@babel/core": "^7.6.4",
      "@babel/preset-env": "^7.6.3",
      "@babel/preset-react": "^7.6.3",
      "babel-eslint": "^10.0.3",
      "babel-loader": "^8.0.6",
      "babel-minify-webpack-plugin": "^0.3.1",
      "babel-plugin-styled-components": "^1.10.6",
      "clean-webpack-plugin": "^3.0.0",
      "css-loader": "^3.2.0",
      "eslint": "^6.6.0",
      "eslint-config-react": "^1.1.7",
      "eslint-loader": "^3.0.2",
      "eslint-plugin-react": "^7.16.0",
      "fibers": "^4.0.2",
      "file-loader": "^4.2.0",
      "html-loader": "^0.5.5",
      "html-webpack-plugin": "^3.2.0",
      "jest": "^24.9.0",
      "jest-styled-components": "^6.3.3",
      "node-sass": "^4.13.0",
      "prettier": "1.18.2",
      "sass": "^1.23.3",
      "sass-loader": "^8.0.0",
      "style-loader": "^1.0.0",
      "webpack": "^4.41.2",
      "webpack-cli": "^3.3.10",
      "webpack-dev-server": "^3.9.0"
   },
   "dependencies": {
      "core-js": "^3.3.6",
      "react": "^16.11.0",
      "react-dom": "^16.11.0",
      "regenerator-runtime": "^0.13.3",
      "styled-components": "^4.4.1"
   }
}
