{
   "name": "react-19-template",
   "version": "0.0.1",
   "description": " A template of a React, Babel, and Webpack app using bleeding-edge packages",
   "main": "index.js",
   "scripts": {
      "start": "npm run lint && webpack-dev-server --open --hot --mode development",
      "build": "npm run lint && webpack --mode production",
      "serve": "http-server ./build -s",
      "serve:loud": "http-server ./build",
      "format": "prettier --write \"src/**/*.js\"",
      "lint": "eslint \"src/**/*.js\""
   },
   "keywords": [
      "react",
      "babel",
      "webpack",
      "es6",
      "es2015",
      "node",
      "template"
   ],
   "author": "Sai Chimata @sai-chai",
   "license": "MIT",
   "devDependencies": {
      "@babel/core": "^7.6.4",
      "@babel/preset-env": "^7.6.3",
      "@babel/preset-react": "^7.6.3",
      "babel-eslint": "^10.0.3",
      "babel-loader": "^8.0.6",
      "babel-plugin-styled-components": "^1.10.6",
      "babel-preset-minify": "^0.5.1",
      "css-loader": "^3.2.0",
      "eslint": "^6.6.0",
      "eslint-config-react": "^1.1.7",
      "eslint-loader": "^3.0.2",
      "eslint-plugin-react": "^7.16.0",
      "file-loader": "^4.2.0",
      "html-loader": "^0.5.5",
      "html-webpack-plugin": "^3.2.0",
      "http-server": "^0.11.1",
      "node-sass": "^4.13.0",
      "prettier": "1.18.2",
      "react": "^16.11.0",
      "react-dom": "^16.11.0",
      "sass-loader": "^8.0.0",
      "style-loader": "^1.0.0",
      "webpack": "^4.41.2",
      "webpack-cli": "^3.3.10",
      "webpack-dev-server": "^3.9.0"
   },
   "dependencies": {}
}
