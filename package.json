{
   "name": "react-19-template",
   "version": "1.0.2",
   "description": " A template of a React, Babel, and Webpack app using bleeding-edge packages",
   "repository": {
      "type": "git",
      "url": "https://github.com/sai-chai/react-19-template.git"
   },
   "main": "index.js",
   "scripts": {
      "prestart": "npm run lint",
      "prebuild": "npm run lint",
      "preversion": "npm run lint",
      "start": "npm run lint && cross-env NODE_ENV=production node ./server.js",
      "start:prod": "npm run lint && cross-env NODE_ENV=production nodemon ./server.js",
      "start:dev": "npm run lint && cross-env NODE_ENV=development nodemon ./server.js --ignore src/*",
      "build": "npm run lint && cross-env NODE_ENV=production webpack",
      "format": "prettier --write \"src/**/*.js\"",
      "lint": "eslint \"src/**/*.js\"",
      "test": "jest"
   },
   "keywords": [
      "react",
      "babel",
      "webpack",
      "es6",
      "es2015",
      "node",
      "template"
   ],
   "author": "Sai Chimata @sai-chai",
   "license": "MIT",
   "devDependencies": {
      "@babel/core": "^7.7.7",
      "@babel/plugin-syntax-dynamic-import": "^7.7.4",
      "@babel/preset-env": "^7.7.7",
      "@babel/preset-react": "^7.7.4",
      "babel-eslint": "^10.0.3",
      "babel-loader": "^8.0.6",
      "babel-minify-webpack-plugin": "^0.3.1",
      "babel-plugin-styled-components": "^1.10.6",
      "clean-webpack-plugin": "^3.0.0",
      "cross-env": "^6.0.3",
      "css-loader": "^3.4.1",
      "eslint": "^6.8.0",
      "eslint-config-react": "^1.1.7",
      "eslint-loader": "^3.0.3",
      "eslint-plugin-react": "^7.17.0",
      "file-loader": "^4.3.0",
      "html-loader": "^0.5.5",
      "html-webpack-plugin": "^5.5.0",
      "jest": "^25.2.4",
      "jest-styled-components": "^6.3.4",
      "node-sass": "^4.13.1",
      "nodemon": "^2.0.2",
      "prettier": "1.18.2",
      "sass": "^1.26.3",
      "sass-loader": "^8.0.2",
      "style-loader": "^1.1.2",
      "webpack": "^4.42.1",
      "webpack-cli": "^3.3.12",
      "webpack-dev-server": "^3.10.3",
      "webpack-hot-middleware": "^2.25.0"
   },
   "dependencies": {
      "core-js": "^3.6.1",
      "react": "^16.12.0",
      "react-dom": "^16.12.0",
      "regenerator-runtime": "^0.13.3",
      "styled-components": "^4.4.1"
   }
}
